  "dependencies": {
    "cross-env": "^5.2.0",
    "laravel-mix": "^4.0.16",
    "sass": "^1.21.0",
    "sass-loader": "^7.1.0",
    "tailwindcss": "^1.0.3",
    "webpack": "^4.33.0",
    "yarn": "^1.16.0"
  }
}
